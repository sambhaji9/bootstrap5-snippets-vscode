{
    "Bootstrap v5 Checkbox Default": {
        "prefix": "bs5-checkbox-default",
        "body": [
            "<div class=\"form-check\">",
                "\t<input class=\"form-check-input\" type=\"checkbox\" value=\"\" id=\"${1:checkboxId}\">",
                "\t<label class=\"form-check-label\" for=\"${1:checkboxId}\">",
                    "\t\t${2:Default checkbox}",
                "\t</label>",
            "</div>"
        ],
        "description": "Creates Bootstrap v5 default Checkbox template"
    },
    "Bootstrap v5 Checkbox checked": {
        "prefix": "bs5-checkbox-checked",
        "body": [
            "<div class=\"form-check\">",
                "\t<input class=\"form-check-input\" type=\"checkbox\" value=\"\" id=\"${1:checkboxId}\" checked>",
                "\t<label class=\"form-check-label\" for=\"${1:checkboxId}\">",
                    "\t\t${2:Checked checkbox}",
                "\t</label>",
            "</div>"
        ],
        "description": "Creates Bootstrap v5 Checked checkbox template"
    },
    "Bootstrap v5 Checkbox disabled": {
        "prefix": "bs5-checkbox-disabled",
        "body": [
            "<div class=\"form-check\">",
                "\t<input class=\"form-check-input\" type=\"checkbox\" value=\"\" id=\"${1:checkboxId}\" disabled>",
                "\t<label class=\"form-check-label\" for=\"${1:checkboxId}\">",
                    "\t\t${2:Disabled checkbox}",
                "\t</label>",
            "</div>"
        ],
        "description": "Creates Bootstrap v5 Disabled checkbox template"
    },
    "Bootstrap v5 Inline checkbox": {
        "prefix": "bs5-checkbox-inline",
        "body": [
            "<div>",
            "\t<div class=\"form-check form-check-inline\">",
                "\t\t<input class=\"form-check-input\" type=\"checkbox\" value=\"option1\" id=\"${1:inlineCheckbox1}\">",
                "\t\t<label class=\"form-check-label\" for=\"${1:inlineCheckbox1}\">",
                    "\t\t\t${2:1}",
                "\t\t</label>",
            "\t</div>",
            "\t<div class=\"form-check form-check-inline\">",
                "\t\t<input class=\"form-check-input\" type=\"checkbox\" value=\"option2\" id=\"${3:inlineCheckbox2}\">",
                "\t\t<label class=\"form-check-label\" for=\"${3:inlineCheckbox2}\">",
                    "\t\t\t${4:2}",
                "\t\t</label>",
            "\t</div>",
            "\t<div class=\"form-check form-check-inline\">",
                "\t\t<input class=\"form-check-input\" type=\"checkbox\" value=\"option3\" id=\"${5:inlineCheckbox3}\" disabled>",
                "\t\t<label class=\"form-check-label\" for=\"${5:inlineCheckbox3}\">",
                    "\t\t\t${6:3 (Disabled)}",
                "\t\t</label>",
            "\t</div>",
            "</div>"
        ],
        "description": "Creates Bootstrap v5 Inline checkbox template"
    },
    "Bootstrap v5 Checkbox no-label": {
        "prefix": "bs5-checkbox-no-label",
        "body": [
            "<div>",
                "\t<input class=\"form-check-input\" type=\"checkbox\" value=\"\" id=\"${1:checkboxNoLabel}\" aria-label=\"...\">",
            "</div>"
        ],
        "description": "Creates Bootstrap v5 checkbox with no-label template"
    },
    "Bootstrap v5 Checkbox stacked": {
        "prefix": "bs5-checkbox-stacked",
        "body": [
            "<div>",
            "\t<div class=\"form-check\">",
            "\t\t<input class=\"form-check-input\" type=\"checkbox\" value=\"\" id=\"${1:defaultCheck1}\">",
            "\t\t<label class=\"form-check-label\" for=\"${1:defaultCheck1}\">",
                "\t\t\t${2:First checkbox}",
            "\t\t</label>",
            "\t</div>",
            "\t<div class=\"form-check\">",
            "\t\t<input class=\"form-check-input\" type=\"checkbox\" value=\"\" id=\"${3:defaultCheck2}\">",
            "\t\t<label class=\"form-check-label\" for=\"${3:defaultCheck2}\">",
                "\t\t\t${4:Second checkbox}",
            "\t\t</label>",
            "\t</div>",
            "\t<div class=\"form-check\">",
            "\t\t<input class=\"form-check-input\" type=\"checkbox\" value=\"\" id=\"${5:defaultCheck3}\" disabled>",
            "\t\t<label class=\"form-check-label\" for=\"${5:defaultCheck3}\">",
                "\t\t\t${6:Disabled checkbox}",
            "\t\t</label>",
            "\t</div>",
            "</div>"
        ],
        "description": "Creates Bootstrap v5 checkbox with no-label template"
    },
    "Bootstrap v5 Toggle checkbox": {
        "prefix": "bs5-toggle-checkbox",
        "body": [
            "<div>",
                "\t<input type=\"checkbox\" class=\"btn-check\" id=\"${1:btnCheckId}\" autocomplete=\"off\">",
                "\t<label class=\"btn btn-${3:primary|secondary|success|danger|warning|info|light|dark}\" for=\"${1:btnCheckId}\">${2:Single toggle }</label>",
            "</div>"
        ],
        "description": "Creates Bootstrap v5 checkbox toggle button template"
    },
    "Bootstrap v5 Toggle checked checkbox": {
        "prefix": "bs5-toggle-checked-checkbox",
        "body": [
            "<div>",
                "\t<input type=\"checkbox\" class=\"btn-check\" id=\"${1:btnCheckId}\" autocomplete=\"off\" checked>",
                "\t<label class=\"btn btn-${3:primary|secondary|success|danger|warning|info|light|dark}\" for=\"${1:btnCheckId}\">${2:Single toggle checked}</label>",
            "</div>"
        ],
        "description": "Creates Bootstrap v5 checkbox checked toggle button template"
    },
    "Bootstrap v5 Toggle disabled checkbox": {
        "prefix": "bs5-toggle-disabled-checkbox",
        "body": [
            "<div>",
                "\t<input type=\"checkbox\" class=\"btn-check\" id=\"${1:btnCheckId}\" autocomplete=\"off\" disabled>",
                "\t<label class=\"btn btn-${3:primary|secondary|success|danger|warning|info|light|dark}\" for=\"${1:btnCheckId}\">${2:Single toggle disabled}</label>",
            "</div>"
        ],
        "description": "Creates Bootstrap v5 checkbox disabled toggle button template"
    }
}
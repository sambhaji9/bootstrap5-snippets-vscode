{
    "Bootstrap v5 Switch Default": {
        "prefix": "bs5-switch-default",
        "body": [
            "<div class=\"form-check form-switch\">",
            "\t<input class=\"form-check-input\" type=\"checkbox\" id=\"${1:flexSwitchCheckDefault}\">",
            "\t<label class=\"form-check-label\" for=\"${1:flexSwitchCheckDefault}\">${2:Default switch checkbox input}</label>",
            "</div>"
        ],
        "description": "Creates Bootstrap v5 default switch"
    },
    "Bootstrap v5 Switch Checked": {
        "prefix": "bs5-switch-checked",
        "body": [
            "<div class=\"form-check form-switch\">",
            "\t<input class=\"form-check-input\" type=\"checkbox\" id=\"${1:flexSwitchCheckChecked}\" checked>",
            "\t<label class=\"form-check-label\" for=\"${1:flexSwitchCheckChecked}\">${2:Checked switch checkbox input}</label>",
            "</div>"
        ],
        "description": "Creates Bootstrap v5 checked switch"
    },
    "Bootstrap v5 Switch Disabled": {
        "prefix": "bs5-switch-disabled",
        "body": [
            "<div class=\"form-check form-switch\">",
            "\t<input class=\"form-check-input\" type=\"checkbox\" id=\"${1:flexSwitchCheckDisabled}\" disabled>",
            "\t<label class=\"form-check-label\" for=\"${1:flexSwitchCheckDisabled}\">${2:Disabled switch checkbox input}</label>",
            "</div>"
        ],
        "description": "Creates Bootstrap v5 disabled switch"
    },
    "Bootstrap v5 Switch Checked Disabled": {
        "prefix": "bs5-switch-checked-disabled",
        "body": [
            "<div class=\"form-check form-switch\">",
            "\t<input class=\"form-check-input\" type=\"checkbox\" id=\"${1:flexSwitchCheckCheckedDisabled}\" checked disabled>",
            "\t<label class=\"form-check-label\" for=\"${1:flexSwitchCheckCheckedDisabled}\">${2:Disabled checked switch checkbox input}</label>",
            "</div>"
        ],
        "description": "Creates Bootstrap v5 checked disabled switch"
    }
}
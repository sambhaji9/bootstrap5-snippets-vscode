{
    "Bootstrap v5 Range Default": {
        "prefix": "bs5-range-default",
        "body": [
            "<label for=\"${1:rangeId}\" class=\"form-label\">${2:Example range}</label>",
            "<input type=\"range\" class=\"form-range\" id=\"${1:rangeId}\">"
        ],
        "description": "Creates Bootstrap v5 range template"
    },
    "Bootstrap v5 Range Disabled": {
        "prefix": "bs5-range-disabled",
        "body": [
            "<label for=\"${1:disableRangeId}\" class=\"form-label\">${2:Disabled range}</label>",
            "<input type=\"range\" class=\"form-range\" id=\"${1:disableRangeId}\" disabled>"
        ],
        "description": "Creates Bootstrap v5 disabled range template"
    },
    "Bootstrap v5 Range Min-Max": {
        "prefix": "bs5-range-min-max",
        "body": [
            "<label for=\"${3:customRangeId}\" class=\"form-label\">${4:Min and Max range}</label>",
            "<input type=\"range\" class=\"form-range\" min=\"${1:0}\" max=\"${2:10}\" id=\"${3:customRangeId}\">"
        ],
        "description": "Creates Bootstrap v5 range template with min and max values"
    },
    "Bootstrap v5 Range Steps": {
        "prefix": "bs5-range-steps",
        "body": [
            "<label for=\"${4:customRangeId}\" class=\"form-label\">${5:Min, Max with steps}</label>",
            "<input type=\"range\" class=\"form-range\" min=\"${1:0}\" max=\"${2:10}\" step=\"${3:0.5}\" id=\"${4:customRangeId}\">"
        ],
        "description": "Creates Bootstrap v5 range template with min, max with intermediate step values"
    }
}
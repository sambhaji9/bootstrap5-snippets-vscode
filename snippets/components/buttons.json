{
    "Bootstrap 5 default button": {
        "prefix": "bs5-button-default",
        "body": [
            "<button type=\"button\" class=\"btn btn-${1:primary|secondary|success|danger|warning|info|light|dark}\">${1:primary|secondary|success|danger|warning|info|light|dark}</button>"
        ],
        "description": "Bootstrap v5 default template for buttons"
    },
    "Bootstrap 5 outline button": {
        "prefix": "bs5-button-outline",
        "body": [
            "<button type=\"button\" class=\"btn btn-outline-${1:primary|secondary|success|danger|warning|info|light|dark}\">${1:primary|secondary|success|danger|warning|info|light|dark}</button>"
        ],
        "description": "Bootstrap v5 template for outline buttons"
    },
    "Bootstrap 5 disabled button": {
        "prefix": "bs5-button-disabled",
        "body": [
            "<button type=\"button\" class=\"btn btn-${1:primary|secondary|success|danger|warning|info|light|dark} btn-${2:lg|sm} disabled\">${1:primary|secondary|success|danger|warning|info|light|dark}</button>"
        ],
        "description": "Bootstrap v5 template for disabled buttons"
    },
    "Bootstrap 5 button sizes (large, small)": {
        "prefix": "bs5-button-sizes",
        "body": [
            "<button type=\"button\" class=\"btn btn-${1:primary|secondary|success|danger|warning|info|light|dark} btn-${2:lg|sm}\">${1:primary|secondary|success|danger|warning|info|light|dark}</button>"
        ],
        "description": "Bootstrap v5 template for disabled buttons"
    },
    "Bootstrap 5 button toggle default": {
        "prefix": "bs5-button-toggle-default",
        "body": [
            "<button type=\"button\" class=\"btn btn-${1:primary|secondary|success|danger|warning|info|light|dark}\" data-bs-toggle=\"button\" autocomplete=\"off\">${1:primary|secondary|success|danger|warning|info|light|dark}</button>"
        ],
        "description": "Bootstrap v5 template for toggle buttons (default)"
    },
    "Bootstrap 5 button active toggle": {
        "prefix": "bs5-button-active-toggle",
        "body": [
            "<button type=\"button\" class=\"btn btn-${1:primary|secondary|success|danger|warning|info|light|dark} active\" data-bs-toggle=\"button\" autocomplete=\"off\" aria-pressed=\"true\">${1:primary|secondary|success|danger|warning|info|light|dark}</button>"
        ],
        "description": "Bootstrap v5 template for active toggle buttons"
    },
    "Bootstrap 5 button disabled toggle": {
        "prefix": "bs5-button-disabled-toggle",
        "body": [
            "<button type=\"button\" class=\"btn btn-${1:primary|secondary|success|danger|warning|info|light|dark}\" disabled data-bs-toggle=\"button\" autocomplete=\"off\">${1:primary|secondary|success|danger|warning|info|light|dark}</button>"
        ],
        "description": "Bootstrap v5 template for active toggle buttons"
    }
}
{
    "name": "bootstrap5-snippets-vscode",
    "displayName": "bootstrap5-snippets-vscode",
    "description": "Simple Bootstrap v5 snippets generator",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.56.0"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "html",
                "path": "./snippets/snippets.json"
            },
            {
                "language": "html",
                "path": "./snippets/components/alerts.json"
            },
            {
                "language": "html",
                "path": "./snippets/components/badges.json"
            },
            {
                "language": "html",
                "path": "./snippets/components/buttons.json"
            },
            {
                "language": "html",
                "path": "./snippets/components/tooltip.json"
            },
            {
                "language": "html",
                "path": "./snippets/components/spinner.json"
            },
            {
                "language": "html",
                "path": "./snippets/components/popover.json"
            },
            {
                "language": "html",
                "path": "./snippets/components/progress.json"
            },
            {
                "language": "html",
                "path": "./snippets/components/offcanvas.json"
            },
            {
                "language": "html",
                "path": "./snippets/forms/select.json"
            },
            {
                "language": "html",
                "path": "./snippets/forms/range.json"
            },
            {
                "language": "html",
                "path": "./snippets/forms/checkbox.json"
            },
            {
                "language": "javascript",
                "path": "./snippets/components/js-snippets.json"
            },
            {
                "language": "javascriptreact",
                "path": "./snippets/components/js-snippets.json"
            },
            {
                "language": "typescriptreact",
                "path": "./snippets/components/js-snippets.json"
            }
        ]
    }
}
